{
	"name": "@freshbooks/api",
	"version": "3.0.0",
	"description": "NodeJS SDK for FreshBooks API",
	"author": "FreshBooks",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com:freshbooks/freshbooks-nodejs-sdk.git"
	},
	"engines": {
		"node": ">=12"
	},
	"main": "dist/index.js",
