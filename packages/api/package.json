{
	"name": "@freshbooks/api",
	"version": "4.0.0",
	"description": "NodeJS SDK for FreshBooks API",
	"author": "FreshBooks",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com:freshbooks/freshbooks-nodejs-sdk.git"
	},
	"engines": {
		"node": ">=14"
	},
	"main": "dist/index.js",
	"type": "commonjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "tsc --declaration",
		"clean": "shx rm -rf ./dist ./coverage",
		"test": "jest --coverage --verbose",
		"lint": "tsc --noEmit && eslint \"{src,__tests__}/**/*.{js,ts}\" --quiet --fix"
	},
	"dependencies": {
		"axios": "^1.2.3",
		"axios-retry": "^4.0.0",
		"luxon": "^3.0.4",
		"typescript": "^5.0.4",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@knodes/typedoc-plugin-pages": "^0.23.1",
		"@types/luxon": "^3.0.1",
		"@types/node": "^20.1.1",
		"axios-mock-adapter": "^1.21.2",
		"typedoc": "^0.25.6"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"collectCoverageFrom": [
			"src/**/*.ts"
		]
	},
	"gitHead": "14f287c3d1ec8de7721adad9d96ac202870a099e"
}
