		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/luxon": "^2.0.3",
		"axios-mock-adapter": "^1.17.0",
		"typedoc": "^0.23.5"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"collectCoverageFrom": [
			"src/**/*.ts"
		]
	},
	"gitHead": "14f287c3d1ec8de7721adad9d96ac202870a099e"
